{
  "name": "future-web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next -p 3000",
    "build": "next build",
    "start": "NODE_ENV=production next start -p 80",
    "export": "next export",
    "update": "rm -rf node_modules && yarn",
    "static:server": "yarn build && yarn export && cd ./out && serve -p 8080",
    "preview": "yarn build && yarn start",
    "lint": "eslint ./ --ext .ts --ext .tsx --fix",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@mui-treasury/components": "^1.5.2",
    "@mui-treasury/styles": "^1.8.0",
    "axios": "^0.21.1",
    "clsx": "^1.1.0",
    "fontfaceobserver": "^2.1.0",
    "moment": "^2.27.0",
    "next": "^9.4.1",
    "ramda": "^0.27.0",
    "react": "latest",
    "react-dom": "latest",
    "react-helmet": "^6.1.0",
    "react-swipeable-views": "^0.13.9",
    "react-swipeable-views-utils": "^0.13.9",
    "react-transcode-render": "^1.0.4",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.13",
    "@types/react": "16.9.38",
    "@types/react-dom": "16.9.8",
    "@types/styled-components": "^5.0.1",
    "babel-plugin-styled-components": "^1.10.7",
    "typescript": "^3.9.5"
  }
}
